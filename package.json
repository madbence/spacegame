  },
  "homepage": "https://github.com/madbence/spacegame#readme",
  "dependencies": {
    "jsonwebtoken": "^7.1.9",
    "koa": "^1.2.4",
    "koa-compose": "^2.4.0",
    "koa-route": "^2.4.2",
    "koa-router": "^5.4.0",
    "mz": "^2.4.0",
    "pg": "^6.1.0",
    "source-map-support": "^0.4.2",
    "stylus": "^0.54.5"
  },
